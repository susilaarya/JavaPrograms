public class IntegerFunctions{
  public static int reverse(int inNumber){
      int revNumber = 0;
    while(inNumber > 0 ){
        revNumber=(revNumber*10)+(inNumber%10);
        inNumber = inNumber/10;
    }
    return revNumber;
  }
  
  public static void main(String[] args){
    System.out.println(reverse(12345));
  }
}
